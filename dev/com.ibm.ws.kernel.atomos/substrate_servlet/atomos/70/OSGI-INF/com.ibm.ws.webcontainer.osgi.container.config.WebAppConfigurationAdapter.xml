  <reference name="resourceRefConfigFactory" cardinality="1..1" interface="com.ibm.ws.resource.ResourceRefConfigFactory" bind="setResourceRefConfigFactory" unbind="unsetResourceRefConfigFactory"/>
  <reference name="servletConfiguratorHelperFactory" cardinality="0..n" policy="dynamic" interface="com.ibm.ws.container.service.config.ServletConfiguratorHelperFactory" bind="setServletConfiguratorHelperFactory" unbind="unsetServletConfiguratorHelperFactory" policy-option="greedy"/>
  <implementation class="com.ibm.ws.webcontainer.osgi.container.config.WebAppConfigurationAdapter"/>
</scr:component>
