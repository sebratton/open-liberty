  <reference name="artifactFactory" cardinality="1..1" interface="com.ibm.wsspi.artifact.factory.ArtifactContainerFactory" bind="setArtifactFactory" unbind="unsetArtifactFactory"/>
  <reference name="moduleFactory" cardinality="1..1" interface="com.ibm.wsspi.adaptable.module.AdaptableModuleFactory" bind="setModuleFactory" unbind="unsetModuleFactory"/>
