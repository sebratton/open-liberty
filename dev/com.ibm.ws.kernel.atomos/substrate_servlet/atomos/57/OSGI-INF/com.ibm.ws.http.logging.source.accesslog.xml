<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" name="com.ibm.ws.http.logging.source.accesslog">
  <property name="service.vendor" type="String" value="IBM"/>
  <service>
    <provide interface="com.ibm.wsspi.collector.manager.Source"/>
  </service>
  <reference name="bufferManager" cardinality="1..1" interface="com.ibm.wsspi.collector.manager.BufferManager" target="(source=com.ibm.ws.http.logging.source.accesslog)" bind="setBufferManager" unbind="unsetBufferManager"/>
  <implementation class="com.ibm.ws.http.logging.source.AccessLogSource"/>
</scr:component>
