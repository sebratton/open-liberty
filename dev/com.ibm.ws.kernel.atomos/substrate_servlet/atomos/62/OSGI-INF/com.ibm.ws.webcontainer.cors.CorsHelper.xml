<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.3.0" name="com.ibm.ws.webcontainer.cors.CorsHelper" configuration-policy="ignore" immediate="true" activate="activate" deactivate="deactivate">
  <property name="service.vendor" type="String" value="IBM"/>
  <service>
    <provide interface="com.ibm.ws.webcontainer.cors.CorsHelper"/>
  </service>
  <reference name="corsConfig" cardinality="0..n" policy="dynamic" interface="com.ibm.ws.webcontainer.cors.config.CorsConfig" bind="setCorsConfig" unbind="unsetCorsConfig" policy-option="greedy"/>
  <implementation class="com.ibm.ws.webcontainer.cors.CorsHelper"/>
</scr:component>
